{
  "AppSettings": {
    "Token": "my-secret-key-that-is-long-enough"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=app.db"
  },
  "Firebase": {
    "ApiKey": "your-api-key",
    "AuthDomain": "your-auth-domain",
    "DatabaseURL": "https://<YOUR_FIREBASE_DATABASE_URL>.firebaseio.com/",
    "ProjectId": "your-project-id",
    "StorageBucket": "your-storage-bucket",
    "MessagingSenderId": "your-messaging-sender-id",
    "AppId": "your-app-id"
  }
}
